<div class="container"> 
  <div class="jumbotron"> 
    <img src="data:image/jpeg;base64,"> 
    <h1> Eat-Da-Burger! </h1>
  </div>

  <div class="row">
    <div class="col-md-6">
      <div class="panel">
        <ul class="list-group">
          {{#each burgers}}
            {{#unless this.devoured}}
              <li class="list-group-item">
                {{this.burger_name}}                
                <form action="burgers/update/{{this.id}}?_method=PUT" method="POST">
                  <input type="hidden" name="devoured" value="true">
                  <button type="submit">DEVOUR</button>
                </form>                
              </li>
            {{/unless}}
          {{/each}}
        </ul>
      </div>
    </div>

    <div class="col-md-6">
      <div class="panel">
        <ul class="list-group">
            {{#each burgers}}
              {{#if this.devoured}}
                <li class="list-group-item">
                  {{this.burger_name}}                            
                </li>
              {{/if}}
            {{/each}}
          </ul>
      </div>
    </div>
  </div>

  <div class="row">
    <div class="col-md-12">
    <h2>Enter a burger to add to the list!</h2>
      <form action="burgers/create" method="POST">
        <div class="form-group">
          <textarea class="form-control input-lg" type="text" name="burger_name" rows="3"> </textarea>
        </div>
        <button class="btn btn-default btn-lg" type="submit">Add!</button>
      </form>
    </div>
  </div>
</div>
